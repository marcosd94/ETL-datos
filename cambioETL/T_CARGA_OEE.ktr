    <sql>SELECT sin.nen_codigo AS &#x22;Codigo Nivel&#x22;,&#xa;sinen.nen_nombre AS &#x22;Descripcion Nivel&#x22;, &#xa;sin.ent_codigo AS &#x22;Codigo Entidad&#x22;,&#xa;sin.ent_nombre AS &#x22;Descripcion Entidad&#x22;,&#xa;orden &#x22;Codigo OEE&#x22;, &#xa;denominacion_unidad AS &#x22;Descripcion OEE&#x22;,&#xa;descripcion_corta AS &#x22;Descripcion Corta&#x22;,&#xa;direccion,&#xa;url,&#xa;telefono,&#xa;vigencia_desde_remuneraciones as fecha_vigencia,&#xa;&#x27;http&#x3a;&#x2f;&#x2f;datos.sfp.gov.py&#x2f;id&#x2f;oee&#x2f;&#x27;&#x7c;&#x7c;sin.nen_codigo&#x7c;&#x7c;&#x27;&#x2f;&#x27;&#x7c;&#x7c;sin.ent_codigo&#x7c;&#x7c;&#x27;&#x2f;&#x27;&#x7c;&#x7c;orden as uri&#xa;&#xa;FROM planificacion.configuracion_uo_cab conf &#xa;&#xa;INNER JOIN planificacion.entidad en ON en.id_entidad&#x3d; conf.id_entidad&#xa;&#xa;INNER JOIN sinarh.sin_entidad sin ON sin.id_sin_entidad &#x3d; en.id_sin_entidad&#xa;&#xa;INNER JOIN sinarh.sin_nivel_entidad sinen ON sinen.nen_codigo &#x3d; sin.nen_codigo &#xa;&#xa;where conf.activo is true order by sin.nen_codigo, sin.ent_codigo , orden</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
