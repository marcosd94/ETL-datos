    <sql>SELECT tmp1.documento_identidad, per.nombres, per.apellidos,per.fecha_nacimiento,tmp1.anho,tmp1.mes,&#xa;nivel,sne.nen_nombre,&#xa;entidad,se.ent_nombre,&#xa;oee,cuc.denominacion_unidad,&#xa;case sexo when &#x27;F&#x27; then &#x27;FEMENINO&#x27; ELSE &#x27;MASCULINO&#x27; end sexo,&#xa;funcion_cumplida,&#xa;case when anho_ingreso is null then 0 else anho_ingreso end as anho_ingreso,&#xa;estado,&#xa;carga_horaria,&#xa;case discapacidad when true then &#x27;SI&#x27; else &#x27;NO&#x27; end as discapacidad,&#xa;&#xa;case when tipo_discapacidad &#x3d; &#x27;0&#x27; and discapacidad &#x3d; true then &#x27;MULTIPLE&#x27;&#xa;when tipo_discapacidad &#x3d; &#x27;1&#x27; and discapacidad &#x3d; true then &#x27;FISICA&#x27;&#xa;when tipo_discapacidad &#x3d; &#x27;2&#x27; and discapacidad &#x3d; true then &#x27;INTELECTUAL&#x27;&#xa;when tipo_discapacidad &#x3d; &#x27;3&#x27; and discapacidad &#x3d; true then &#x27;PSICOSOCIAL&#x27;&#xa;when tipo_discapacidad &#x3d; &#x27;4&#x27; and discapacidad &#x3d; true then &#x27;AUDITIVA&#x27;&#xa;when tipo_discapacidad &#x3d; &#x27;5&#x27; and discapacidad &#x3d; true then &#x27;VISUAL&#x27;&#xa;when tipo_discapacidad is null and discapacidad &#x3d; false then &#x27; &#x27;&#xa;when tipo_discapacidad is null and discapacidad &#x3d; true then &#x27;NO ESPECIFICA&#x27;&#xa;end as tipo_discapacidad,&#xa;&#xa;obj_codigo,fuente_financiamiento,&#xa;case when presupuestado is null then 0 else presupuestado end as presupuestado,&#xa;case when devengado is null then 0 else devengado end as devengado,&#xa;cargo,&#xa;concepto,&#xa;case when linea is null or linea &#x3d; 0 then &#x27;SIN L&#xcd;NEA&#x27; else linea&#x3a;&#x3a;varchar end as linea,&#xa;case when categoria is null then &#x27;SIN CATEGOR&#xcd;A&#x27; else categoria end as categoria,&#xa;case movimiento &#xa;when &#x27;TT&#x27; then &#x27;TT - TRANSLADO TEMPORAL&#x27;&#xa;when &#x27;TL&#x27; then &#x27;TL - TRANSLADO DEFINITIVO CON L&#xcd;NEA&#x27;&#xa;when &#x27;A&#x27; then