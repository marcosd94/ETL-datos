    <sql>SELECT con.identificador_concurso, con.identificador_concurso_puesto,&#xa;con.nivel,con.descripcion_nivel,&#xa;con.entidad,con.descripcion_entidad,&#xa;con.oee,con.descripcion_oee,&#xa;tipos_concurso,&#xa;objeto_gasto, concepto ,&#xa;clasificacion_ocupacional,&#xa;categoria, cargo, puesto,&#xa;vacancia, tipo_funcionario,&#xa;localidad,domicilio,&#xa;salario, beneficios_adicionales,&#xa;con.estado as estado_concurso,&#xa;fuente_financiamiento,&#xa;inicio_publicacion,&#xa;fin_publicacion,&#xa;con.uri,&#xa;--null as perfil_matriz_postulacion, null as informacion_postulacion,null as postularse, null as inicio_postulacion, null as fin_postulacion--&#xa;post.perfil_matriz as perfil_matriz_postulacion , post.informacion as informacion_postulacion, post.postularse, post.inicio_postulacion,post.fin_postulacion,&#xa;eva.estado as estado_evaluacion, eva.cant_postulantes, eva.perfil_matriz as perfil_matriz_evaluacion , eva.proceso as proceso_evaluacion, eva.inicio_evaluacion, eva.fin_evaluacion,&#xa;adju.cedula,adju.nombre, adju.apellido,adju.fecha_nacimiento, &#xa;adju.fecha_adjudicacion, adju.proceso as proceso_adjudicacion,&#xa;adju.adjudicados, adju.puntaje, adju.perfil_matriz as perfil_matriz_adjudicacion&#xa;&#xa;FROM opendata.concursos con&#xa;&#xa;left JOIN opendata.postulacion post ON post.identificador_concurso_puesto &#x3d; con.identificador_concurso_puesto&#xa;left JOIN opendata.evaluacion eva ON eva.identificador_concurso_puesto &#x3d; con.identificador_concurso_puesto &#xa;LEFT JOIN opendata.adjudicacion adju ON adju.identificador_concurso_puesto &#x3d; con.identificador_concurso_puesto&#xa;&#xa;</sql>
    <limit>0</limit>
    <lookup/>
