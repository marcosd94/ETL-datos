    <connection>sfp</connection>
    <sql>select &#x2a; from &#x28;select co.id_concurso, &#xa;cpg.id_concurso_puesto_agr, &#xa;en.nen_codigo,UPPER&#x28;sne.nen_nombre&#x29; as nen_nombre,&#xa;en.ent_codigo,UPPER&#x28;sen.ent_nombre&#x29; as ent_nombre,&#xa;oee.orden,UPPER&#x28;oee.denominacion_unidad&#x29; as OEE, &#xa;sel.puntaje_realizado as Puntaje,&#xa;per.documento_identidad,&#xa;UPPER&#x28;per.nombres&#x29; as nombres,&#xa;UPPER&#x28;per.apellidos&#x29; as apellidos,&#xa;per.fecha_nacimiento,&#xa;cpg.fecha_adjudicacion,&#xa;UPPER&#x28;cpg.descripcion_grupo&#x29; as descripcion_grupo,&#xa;UPPER&#x28;de.descripcion&#x29; as tipo_concurso,&#xa;cpg.estado,&#xa;UPPER&#x28;re.desc_larga&#x29; as desc_larga, &#xa;cantidad_puestos, &#xa;cantidades.postulantes,&#xa;&#x27;https&#x3a;&#x2f;&#x2f;www.paraguayconcursa.gov.py&#x2f;sicca&#x2f;seleccion&#x2f;buscarConcurso&#x2f;adjudicados&#x2f;adjudicados.seam&#x3f;idConcursoPuestoAgr&#x3d;&#x27;&#x7c;&#x7c;cpg.id_concurso_puesto_agr&#x7c;&#x7c;&#x27;&#x26;firstResult&#x3d;0&#x26;paginaActual&#x3d;adjudicados&#x26;from&#x3d;Portal.seam&#x26;sugestion&#x3d;&#x26;sugestionGrupo&#x3d;&#x26;logic&#x3d;and&#x26;actionMethod&#x3d;seleccion&#x25;2FbuscarConcurso&#x25;2Fadjudicados&#x25;2Fadjudicados.xhtml&#x25;3AconcursoListFormController.imprimirPerfilMatriz&#x25;28&#x25;29&#x26;cid&#x3d;22570&#x27; as perfil_matriz,&#xa;&#x27;https&#x3a;&#x2f;&#x2f;www.paraguayconcursa.gov.py&#x2f;sicca&#x2f;seleccion&#x2f;verPostulacion&#x2f;verPostulacionPortal.seam&#x3f;idConcursoPuestoAgr&#x3d;&#x27;&#x7c;&#x7c;cpg.id_concurso_puesto_agr&#x7c;&#x7c;&#x27;&#x26;idConcurso&#x3d;&#x27;&#x7c;&#x7c;co.id_concurso&#x7c;&#x7c;&#x27;&#x26;from&#x3d;seleccion&#x25;2FbuscarConcurso&#x25;2Fadjudicados&#x25;2Fadjudicados&#x26;sugestion&#x3d;&#x26;sugestionGrupo&#x3d;&#x26;firstResult&#x3d;0&#x26;cid&#x3d;22570&#x27; as proceso,&#xa;&#x27;https&#x3a;&#x2f;&#x2f;www.paraguayconcursa.gov.py&#x2f;sicca&#x2f;seleccion&#x2f;verPostulacion&#x2f;verPostulacionPortal.seam&#x3f;&#x26;imprimirCU&#x3d;CU_161&#x26;idConcursoPuestoAgr&#x3d;&#x27;&#x7c;&#x7c;cpg.id_concurso_puesto_agr as adjudicados&#xa;&#xa;FROM seleccion.concurso_puesto_agr cpg&#xa;INNER JOIN seleccion.concurso co ON cpg.id_concurso &#x3d; co.id_concurso&#xa;INNER JOIN planificacion.configuracion_uo_cab oee ON oee.id_configuracion_uo &#x3d; co.id_configuracion_uo&#xa;INNER JOIN seleccion.fechas_grupo_puesto fgp ON fgp.id_concurso_puesto_agr &#x3d; cpg.id_concurso_puesto_agr&#xa;INNER JOIN seleccion.referencias re ON re.valor_num &#x3d; cpg.estado AND re.dominio LIKE &#x27;ESTADOS_GRUPO&#x27;&#xa;INNER JOIN seleccion.datos_especificos de ON co.id_datos_esp_tipo_conc &#x3d; de.id_datos_especificos&#xa;INNER JOIN planificacion.entidad en ON en.id_entidad &#x3d; oee.id_entidad &#xa;INNER JOIN sinarh.sin_entidad sen ON sen.id_sin_entidad &#x3d; en.id_sin_entidad &#xa;INNER JOIN sinarh.sin_nivel_entidad sne ON sne.nen_codigo &#x3d; sen.nen_codigo&#xa;&#xa;LEFT JOIN &#x28;SELECT &#x2a; FROM seleccion.eval_referencial_postulante eval WHERE &#xa;eval.seleccionado_replica &#x3d; TRUE AND eval.seleccionado_replica IS NOT NULL &#xa;AND EVAL.ACTIVO &#x3d; TRUE&#xa;AND EVAL.ID_EXCEPCION IS NULL&#xa;AND &#x28;EVAL.EXCLUIDO is FALSE or EVAL.EXCLUIDO is NULL&#x29; &#x2f;&#x2a;and id_concurso_puesto_agr &#x3d; 32&#x2a;&#x2f;&#x29; sel on sel.id_concurso_puesto_agr &#x3d; cpg.id_concurso_puesto_agr&#xa;&#xa;LEFT JOIN seleccion.postulacion sel_p ON sel_p.id_postulacion &#x3d; sel.id_postulacion&#xa;LEFT JOIN seleccion.persona_postulante per ON per.id_persona_postulante &#x3d; sel_p.id_persona_postulante&#xa;&#xa;LEFT JOIN &#x28;&#xa;select count&#x28;&#x2a;&#x29; as postulantes, id_concurso_puesto_agr&#xa;from seleccion.postulacion&#xa;where activo &#x3d; true&#xa;group by id_concurso_puesto_agr&#x29; cantidades on  cpg.id_concurso_puesto_agr &#x3d; cantidades.id_concurso_puesto_agr&#xa;&#xa;WHERE cpg.activo &#x3d; TRUE AND co.activo &#x3d; TRUE &#xa;&#xa;				AND cpg.fecha_adjudicacion is not null &#xa;				AND  co.activo &#x3d; true&#xa;				OR &#x28;cpg.estado IN&#x28;select valor_num from seleccion.referencias &#xa;				where&#xa;				dominio &#x3d; &#x27;ESTADOS_GRUPO&#x27; &#xa;				and   &#x28;desc_abrev &#x3d; &#x27;GRUPO DESIERTO&#x27; &#xa;				OR desc_abrev &#x3d; &#x27;DESIERTO_CON_DOC&#x27; &#xa;				OR desc_abrev &#x3d; &#x27;PUBLICACION_FINALIZADO&#x27; &#x29;&#x29;&#x29;&#xa;AND not cpg.estado in &#x28;35,38&#x29;&#xa;ORDER BY co.id_concurso, cpg.id_concurso_puesto_agr&#x29; as a where documento_identidad is not null</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
