    <sql>select c.&#x2a;, c.cantidad_altas_concurso&#x3a;&#x3a;float &#x2f; c.cantidad_altas as indice_concursabilidad&#xa;from &#x28;&#xa;select &#xa;f.anho, &#xa;&#x28;f.mes&#x2f;4&#x29;&#x2b;1 as trimestre, &#xa;f.nivel,&#xa;o.descripcion_nivel, &#xa;f.entidad, &#xa;o.descripcion_entidad, &#xa;f.oee, &#xa;o.descripcion_oee, &#xa;count&#x28;a.cedula&#x29; as cantidad_altas_concurso,&#xa;count&#x28;f.documento&#x29; as cantidad_altas&#xa;from opendata.detalles_funcionarios f&#xa;left join opendata.adjudicacion a&#xa;on extract&#x28;year from a.fecha_adjudicacion&#x29; &#x3d; f.anho &#xa;and a.cedula &#x3d; f.documento&#xa;and a.nivel &#x3d; f.nivel &#xa;and a.entidad &#x3d; f.entidad &#xa;and a.oee &#x3d; f.oee&#xa;join opendata.oee o&#xa;on f.nivel &#x3d; o.codigo_nivel &#xa;and f.entidad &#x3d; o.codigo_entidad &#xa;and f.oee &#x3d; o.codigo_oee&#xa;where &#xa;-- categorias de la carrera administrativa &#x28;tomamos del sueldo su categoria&#x29;&#xa;f.objeto_gasto in &#x28;111, 112&#x29; &#xa;and substring&#x28;f.categoria,1,1&#x29; in &#x28;&#x27;A&#x27;,&#x27;B&#x27;,&#x27;C&#x27;,&#x27;D&#x27;,&#x27;E&#x27;,&#x27;F&#x27;,&#x27;G&#x27;&#x29;&#xa;-- que fueron dados de alta&#xa;and f.movimiento like &#x27;&#x25;ALTA&#x25;&#x27; -- &#x22;A - ALTA &#x28;INCORPORACI&#xd3;N&#x29;&#x22;&#xa;-- se empieza a computar desde el anho 2015&#xa;and f.anho_ingreso &#x3e;&#x3d; 2015&#xa;group by 1,2,3,4,5,6,7,8&#xa;order by 1,3,4,5,2&#xa;&#x29; c</sql>
