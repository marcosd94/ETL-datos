    <sql>select&#xa;co.id_concurso, cpg.id_concurso_puesto_agr, &#xa;en.nen_codigo,UPPER&#x28;sne.nen_nombre&#x29; as nen_nombre,&#xa;en.ent_codigo,UPPER&#x28;sen.ent_nombre&#x29; as ent_nombre,&#xa;oee.orden,UPPER&#x28;oee.denominacion_unidad&#x29; as OEE, &#xa;UPPER&#x28;de.descripcion&#x29; as tipo_concurso, &#xa;UPPER&#x28;cpg.modalidad&#x29; as tipo_funcionario,&#xa;UPPER&#x28;plan.descripcion&#x29; as cargo,&#xa;UPPER&#x28;cpg.descripcion_grupo&#x29; as puesto,&#xa;UPPER&#x28;cpt_gen.denominacion&#x29; as clasificacion_ocupacional,&#xa;gcp.monto, &#xa;UPPER&#x28;cpg.fuente_financiacion&#x29; as fuente_financiacion, -- solo se cuenta con este campo tipo text&#xa;gcp.obj_codigo,&#xa;UPPER&#x28;obj.obj_nombre&#x29; as obj_nombre,&#xa;gcp.categoria,&#xa;UPPER&#x28;cpg.lugar&#x29; as domicilio,&#xa;UPPER&#x28;ciu.descripcion&#x29; as localidad,&#xa;UPPER&#x28;cpg.otros_beneficios&#x29; as benificios_adicionales,&#xa;cantidad_puestos, &#xa;UPPER&#x28;re.desc_larga&#x29; AS estado_especifico,&#xa;to_char&#x28;fgp.fecha_publicacion_desde &#x2b; fgp.hora_publicacion_desde,&#x27;YYYY-MM-DD hh24&#x3a;mi&#x3a;ss&#x27;&#x29;&#x3a;&#x3a;timestamp as inicio_publicacion,&#xa;to_char&#x28;fgp.fecha_publicacion_hasta &#x2b; fgp.hora_publicacion_hasta,&#x27;YYYY-MM-DD hh24&#x3a;mi&#x3a;ss&#x27;&#x29;&#x3a;&#x3a;timestamp as fin_publicacion,&#xa;&#x27;http&#x3a;&#x2f;&#x2f;datos.sfp.gov.py&#x2f;id&#x2f;concursos&#x2f;&#x27;&#x7c;&#x7c;en.nen_codigo&#x7c;&#x7c;&#x27;&#x2f;&#x27;&#x7c;&#x7c;en.ent_codigo&#x7c;&#x7c;&#x27;&#x2f;&#x27;&#x7c;&#x7c;oee.orden&#x7c;&#x7c;&#x27;&#x2f;&#x27;&#x7c;&#x7c;cpg.id_concurso_puesto_agr as uri,&#xa;cpg.estado,&#xa;&#xa;case cpg.estado&#xa;--POSTULACION&#xa;when 15 then &#x27;POSTULACI&#xd3;N&#x27;&#xa;when 13 then &#x27;POSTULACI&#xd3;N&#x27;&#xa;when 1000 then &#x27;POSTULACI&#xd3;N&#x27;&#xa;&#xa;--EVALUACION&#xa;when 22 then &#x27;EVALUACI&#xd3;N&#x27;&#xa;when 19 then &#x27;EVALUACI&#xd3;N&#x27;&#xa;when 26 then &#x27;EVALUACI&#xd3;N&#x27;&#xa